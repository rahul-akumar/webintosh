    "firebase": "^12.1.0",
    "nuxt": "^4.0.3",
    "nuxt-gtag": "^4.0.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.8.0",
    "@nuxt/test-utils": "3.19.2",
